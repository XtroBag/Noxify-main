{
  "name": "noxify",
  "version": "1.4.0",
  "description": "Basic discord bot using discord.js with typescript",
  "main": "dist/index.js",
  "types": "environment.d.ts",
  "scripts": {
    "start:prod": "node dist/src/index.js",
    "clean": "rimraf dist",
    "build": "tsc",
    "start": "npm run clean && npm run build && npm run start:prod",
    "deploy": "npm run clean && npm run build && node dist/src/deploy.js"
  },
  "directories": {
    "lib": "src"
  },
  "type": "module",
  "author": "XtroBag",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.2.0",
    "axios": "^1.4.0",
    "chalk": "^5.2.0",
    "discord.js": "^14.13.0",
    "dotenv": "^16.0.3",
    "fs": "^0.0.1-security",
    "mongoose": "^7.2.0",
    "ms": "^2.1.3",
    "path": "^0.12.7",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@types/ms": "^0.7.31",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.41.0",
    "prisma": "^5.2.0",
    "rimraf": "^5.0.1",
    "tsx": "^3.12.7",
    "typescript": "^5.0.3"
  },
  "engines": {
    "node": ">=16.9.0"
  },
  "prisma": {
    "schema": "./database/schema.prisma"
  }
}

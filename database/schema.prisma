generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:dev.db"
}

model Guild {
  id      Int     @id @default(autoincrement())
  name    String
  guildId String 
  prefix  String  @default("?")
  mode    Boolean @default(false)
  afkMembers     AFK[]
}

model AFK {
  id        Int   @id @default(autoincrement())
  reason    String
  timestamp Int
  guild     Guild @relation(fields: [id], references: [id])
}

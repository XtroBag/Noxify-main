generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:dev.db"
}

model Guild {
  guildID    Int     @id @default(autoincrement())
  name       String
  id         String  @unique
  prefix     String  @default("?")
  mode       Boolean @default(false)
  afkMembers AFK[]
}

model AFK {
  afkID     Int    @id @default(autoincrement())
  reason    String
  userID    String @unique
  guild     Guild  @relation(fields: [afkID], references: [guildID])
}

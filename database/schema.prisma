datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model afk {
  guildID   String
  userID    String
  reason    String
  mentions  Int
  timestamp String

  @@unique([guildID, userID])
}


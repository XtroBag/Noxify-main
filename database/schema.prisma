generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:dev.db"
}

model Guild {
  id      Int     @id @default(autoincrement())
  name    String
  guildID String  @unique
  prefix  String  @default("?")
  mode    Boolean @default(false)
  afk     AFK[]
}

model AFK {
  id     Int    @id @default(autoincrement())
  reason String
  userID String @unique
  guild  Guild  @relation(fields: [id], references: [id])
}

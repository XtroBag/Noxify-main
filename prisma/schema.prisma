generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Toggle {
  id      String  @id @default(auto()) @map("_id") @db.ObjectId
  guildId String
  mode    Boolean
}

model Guilds {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  guildName String
  guildId   String
  prefix    String @default("nx.")
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model guild {
  guildID   String @unique
  guildName String
  logsID String
  mode Boolean @default(false)
}

model afk {
  guildID   String @unique
  userID    String @unique
  reason    String
  mentions  Int
  timestamp String
}


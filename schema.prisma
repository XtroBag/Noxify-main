datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

// Guild Model
model Guild {
  id          Int          @id @default(autoincrement())
  guildID     String       @unique
  guildName   String
  logsID      String
  mode        Boolean      @default(false)
}

// Afk Model
model afk {
  guildID   String @unique
  userID    String @unique
  reason    String
  mentions  Int
  timestamp String
}

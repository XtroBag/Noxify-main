datasource db {
  provider     = "mysql"
  url          = env("PLANETSCALE_URI")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model Guild {
  id          Int          @id @default(autoincrement())
  guildID     String       @unique
  guildName   String
  logsID      String
  mode        Boolean      @default(false)

  @@index([id])
}

model afk {
  guildID   String @unique
  userID    String @unique
  reason    String
  mentions  Int
  timestamp String
}
